<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		
		<bean class="converters.AuthorToStringConverter" />
		<bean class="converters.StringToAuthorConverter" />
		
		<bean class="converters.PublisherToStringConverter" />
		<bean class="converters.StringToPublisherConverter" />
		
		<bean class="converters.ComicToStringConverter" />
		<bean class="converters.StringToComicConverter" />
		
		<bean class="converters.ComicCharacterToStringConverter" />
		<bean class="converters.StringToComicCharacterConverter" />
		
		<bean class="converters.StringToAuthorityConverter" />
		<bean class="converters.AuthorityToStringConverter" />
		
		<bean class="converters.StringToCommentConverter" />
		<bean class="converters.CommentToStringConverter" />
		
		<bean class="converters.StringToVolumeConverter" />
		<bean class="converters.VolumeToStringConverter" />
		
		<bean class="converters.StringToUserConverter" />
		<bean class="converters.UserToStringConverter" />
		
		<bean class="converters.StringToMessageFolderConverter" />
		<bean class="converters.MessageFolderToStringConverter" />
		
		<bean class="converters.StringToDirectMessageConverter" />
		<bean class="converters.DirectMessageToStringConverter" />
		
		<bean class="converters.StringToUserComicConverter" />
		<bean class="converters.UserComicToStringConverter" />
		
		<bean class="converters.StringToComicComicCharacterConverter" />
		<bean class="converters.ComicComicCharacterToStringConverter" />
		
		
		
		
		
		
		
		
	</util:list>
	
</beans>